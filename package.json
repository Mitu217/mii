{
  "name": "ortensia",
  "version": "0.0.1",
  "description": "management media release calendar",
  "private": true,
  "scripts": {
    "build": "./node_modules/.bin/tsc",
    "dev": "./node_modules/.bin/ts-node ./src/index.ts",
    "start": "npm run build && ./node_modules/.bin/cross-env NODE_ENV=production node index.js",
    "deploy": "npm run build && cp package.json ./dest && gcloud beta functions deploy ortensia --runtime nodejs8 --trigger-http --region asia-northeast1 --source=./dest",
    "test": "npm run build && mocha --timeout 10000 --reporter spec"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Mitu217/ortensia.git"
  },
  "author": "Mitu217",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Mitu217/ortensia/issues"
  },
  "homepage": "https://github.com/Mitu217/ortensia#readme",
  "dependencies": {
    "@google-cloud/datastore": "^1.4.1",
    "cheerio": "^1.0.0-rc.2",
    "cron": "^1.4.0",
    "cross-env": "^5.2.0",
    "express": "^4.16.3",
    "mkdirp": "^0.5.1",
    "node-fetch": "^2.2.0",
    "node-uuid": "^1.4.8"
  },
  "devDependencies": {
    "@types/cron": "^1.3.0",
    "@types/express": "^4.16.0",
    "@types/node": "^10.7.1",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "node-mocks-http": "^1.7.0",
    "proxyquire": "^2.0.1",
    "sinon": "^6.1.5",
    "sinon-chai": "^3.2.0",
    "ts-node": "^7.0.1",
    "typescript": "^2.9.2",
    "vinyl-source-stream": "^2.0.0"
  }
}
